            corner.Parent = warnFrame

            local warnLabel = Instance.new("TextLabel")
            warnLabel.Size = UDim2.new(1, -30, 1, 0)
            warnLabel.Position = UDim2.new(0, 10, 0, 0)
            warnLabel.BackgroundTransparency = 1
