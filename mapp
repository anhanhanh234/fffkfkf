    if Toogle == true and BetterLoopOperator == false then

       BetterLoopOperator = true

       pcall(function()

       loadstring(script)()

       end)

       BetterLoopOperator = false

    end

    end)

 elseif type == "Switch" then

    local TextLabel = Instance.new("TextLabel") 

